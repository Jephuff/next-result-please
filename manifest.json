{
    "manifest_version": 2,
    
    "name": "search pager",
    "description": "tab through google search results",
    "version": "0.0",

    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["https://www.google.ca/*", "https://www.google.com/*"],
        "js":      ["index.js"]
    }],
    "permissions": [
        "tabs"
    ],
    "commands": {
        "next_link": {
            "suggested_key": {
                "default": "Ctrl+Shift+0"
            },
            "description": "go to next link",
            "global": true
        },
        "previous_link": {
            "suggested_key": {
                "default": "Ctrl+Shift+9"
            },
            "description": "go to previous link",
            "global": true
        }
    }
}


  