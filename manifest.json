{
    "manifest_version": 2,
    
    "name": "search pager",
    "description": "tab through google search results",
    "version": "0.1",

    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["https://www.google.ca/search*", "https://www.google.com/search*"],
        "js":      ["index.js"]
    }],
    "permissions": [
        "tabs"
    ],
    "commands": {
        "next_link": {
            "suggested_key": {
                "default": "Ctrl+Shift+X"
            },
            "description": "go to next link"
        },
        "previous_link": {
            "suggested_key": {
                "default": "Ctrl+Shift+Z"
            },
            "description": "go to previous link"
        }
    }
}


  